{
  "metadata": [
    {
      "src": [
        {
          "files":"NativePlugins/IOSNativePro/**.cs",
          "src":"../../Plugins/Assets/Plugins/StansAssets/"
        }
      ],
      "dest": "ios-native-api"
    },
    {
      "src": [
        {
          "files":"NativePlugins/AndroidNativePro/**.cs",
          "src":"../../Plugins/Assets/Plugins/StansAssets/"
        }
      ],
      "dest": "android-native-api"
    },
     {
      "src": [
        {
          "files":"CrossPlatform/**.cs",
          "src":"../../Plugins/Assets/Plugins/StansAssets/"
        }
      ],
      "dest": "ultimate-mobile-api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "ios-native-api/**.yml",
          "android-native-api/**.yml"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "**/bin/**",
          "**/obj/**"
        ]
      }
    ],
    "dest": "../../com.stansassets.ultimate-mobile__gh-pages",
    "template": [
      "default"
    ]
  }
}
